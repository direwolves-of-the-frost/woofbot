{
	"defaultSeverity": "error",
	"extends": [
		"tslint:recommended"
	],
	"rules": {
		"indent": [true, "tabs"],
		"max-line-length": false,
		"interface-name": false,
		"import-spacing": true,
		"ordered-imports": true,
		"ban-types": false,
		"no-bitwise": false,
		"curly": true,
		"member-ordering": false,
		"object-literal-sort-keys": false,
		"max-classes-per-file": true,
		"no-console": false,
		"quotemark": [true, "single", "avoid-escape"]
	}
}
